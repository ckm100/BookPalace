<div class="notification-container"> <% if (allData.requestTo.length !==0){%> <p class="pending-request">You have <%=allData.requestTo.length %> pending book request</p><%}%> <% if (allData.requestFrom.length !==0){%> <% allData.requestFrom.forEach(function(val){%> <p class="pending-approval"> <%=val.requestUserName%> requested a book titled <%=val.bookName%> from your collection &nbsp;&nbsp;&nbsp;&nbsp; <i class="fa fa-check-circle accept-request" data-userId="<%=val.requestUserId%>" data-bookId="<%=val.bookId%>"></i> &nbsp;&nbsp; <i class="fa fa-times-circle decline-request" data-userId="<%=val.requestUserId%>" data-bookId="<%=val.bookId%>"></i></p><%}) %> <%}%></div><% if (data.length===0){%> <h3>You have no books, click the add book button below to start adding your books</h3> <div class="books-container"> <div class="add-book-button"> <h3>Add Book</h3> <div class="hover-bg"></div></div></div><%}else{%> <h1>Your Book Collection</h1> <div class="books-container"> <% data.forEach(function(val){%> <div class="book"> <img src="<%=val.thumbnail%>" alt="<%=val.title%>" class="book-thumbnail"> <div class="book-info"> <p><strong>Title:</strong> <%=val.title%> </p><p><strong>Authors:</strong> <%=val.authors.join(", ") %> </p><p><strong>Publisher:</strong> <%=val.publisher%> </p><p><strong>Published Date:</strong> <%=val.publishedDate%> </p><p><strong>Book Info:</strong> <a href="<%=val.link%>" target="_blank" class="book-info-link">Here</a></p></div></div><%}) %> <div class="add-book-button"> <h3>Add Book</h3> <div class="hover-bg"></div></div></div><%}%>